<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="46.0" minWidth="100.0"
	prefHeight="471.0" prefWidth="288.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.Chat.MyController">

	<children>
		<Button fx:id="chatButton" layoutX="208.0" layoutY="385.0"
			mnemonicParsing="false" onAction="#addChat" text="Chat" />
		<TableView fx:id="tableView" layoutX="7.0" layoutY="28.0"
			onMouseClicked="#showinfo" prefHeight="350.0" prefWidth="274.0">
			<columns>
				<TableColumn fx:id="memo" prefWidth="199.0" text="memo" />
				<TableColumn fx:id="date" prefWidth="74.0" text="date" />
			</columns>
		</TableView>
		<TextField fx:id="textAmu" layoutX="37.0" layoutY="385.0"
			prefHeight="26.0" prefWidth="171.0" />
		<Button fx:id="exitbutton" layoutX="208.0" layoutY="428.0"
			mnemonicParsing="false" onAction="#out" text="나가기" />
	</children>
</AnchorPane>
